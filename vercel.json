{
  "version": 2,
  "builds": [
    {
      "src": "Amar_Recipe/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "outputDirectory": "Amar_Recipe/dist",
  "routes": [
    {
      "src": "/(?!api)(?!.json)(?!.svg)(?!.png)(?!.jpg)(?!.jpeg)(?!.gif)(?!.css)(?!.js).*",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://amar-recipes.infinityfreeapp.com/api"
  }
}
